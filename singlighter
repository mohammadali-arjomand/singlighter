const command = process.argv[2];

switch (command) {
    case "new": {
        if (process.argv.length > 4)
            console.error("You must set a name for your project");
        else
            newCmd(process.argv[3]);
        break;
    }
    default: {
        console.error(`Bad usage\n'${command}' is not a command.`);
    }
}

